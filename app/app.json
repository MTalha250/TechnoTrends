{
  "expo": {
    "name": "Techno",
    "slug": "techno-trends",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "UIBackgroundModes": [
          "background-processing",
          "background-fetch"
        ]
      },
      "bundleIdentifier": "com.talhaad126.technotrends"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-small.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.talhaad126.technotrends",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE"
      ]
    },
    "notification": {
      "icon": "./assets/images/icon-small.png",
      "color": "#A82F39",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "TechnoTrends"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon-small.png",
          "color": "#A82F39",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d4321f7-71df-4a60-a087-0ad308d69dca"
      }
    }
  }
}
